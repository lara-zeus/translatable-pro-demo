{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "awcodes/filament-tiptap-editor": "^3.0",
        "fakerphp/faker": "^1.23",
        "filament/filament": "^3.2",
        "jibaymcs/filament-tour": "dev-laravel-12",
        "lara-zeus/translatable-pro": "^1.0",
        "laravel/framework": "^12.0",
        "ibrahim-bougaoua/filaprogress": "dev-laravel-12",
        "sentry/sentry-laravel": "^4.9"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/tinker": "^2.9",
        "laravel/pint": "^1.13",
        "nunomaduro/collision": "^8.0",
        "spatie/laravel-ignition": "^2.4",
        "barryvdh/laravel-debugbar": "^3.9",
        "larastan/larastan": "^3.1",
        "laravel-lang/common": "^6.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "github",
            "url": "git@github.com:lara-zeus/translatable-pro.git"
        },
        {
            "type": "github",
            "url": "https://github.com/atmonshi/filament-tour"
        },
        {
            "type": "github",
            "url": "https://github.com/atmonshi/filaprogress"
        }
    ]
}
